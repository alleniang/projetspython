    "C, H = 50, 30\n",
    "def calcul(C, H):\n",
    "    d=input(\"Veuillez saisir la séquence:\")\n",
    "    d=d.split(\",\")\n",
    "    my_set = set(d)\n",
    "    my_new_set = set()\n",
    "    for D in my_set:\n",
    "        D=int(D)\n",
    "        my_new_set.add(round((2 * C * D)/H))\n",
    "    return my_new_set\n",
    "calcul(C, H)"
   ]
